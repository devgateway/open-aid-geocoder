{
  "presets": ["react", "es2015","stage-0","stage-1","stage-2","stage-3"],
  "env": {
      "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }, {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }
          ]
        }]
      ]
    }
  }
}